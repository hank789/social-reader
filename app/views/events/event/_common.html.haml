.event-title
  %span.author_name= link_to_author event
  %span.event_label{class: event.action_name}= event_action_name(event)
.event-body
  %strong= gfm event.post.title

- if event.post.photos.count >= 1
  .event-body
    .event-note
      - event.post.photos.each do |photo|
        = image_tag photo.image.url
